{
  "name": "modulajs",
  "version": "1.0.0-rc.1",
  "description": "A modularization framework to manage application states using an immutable model tree",
  "main": "index.js",
  "author": "eng-ui@freewheel.tv",
  "license": "Apache-2.0",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf lib && babel src --ignore **/__tests__/** --out-dir lib",
    "lint": "eslint --cache --cache-location node_modules/.cache --ext js .",
    "lint_one": "eslint",
    "precommit": "lint-staged",
    "test": "babel-node node_modules/.bin/_mocha **/__tests__/*-test.js --harmony_proxies ",
    "test_cov": "rimraf coverage && babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- **/__tests__/*-test.js --harmony_proxies",
    "test_one": "babel-node node_modules/.bin/_mocha --harmony_proxies"
  },
  "homepage": "https://github.com/Comcast/modulajs",
  "bugs": {
    "url": "https://github.com/Comcast/modulajs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Comcast/modulajs.git"
  },
  "keywords": [
    "modula",
    "modularization",
    "modulajs",
    "application-state",
    "state-management",
    "immutable",
    "model",
    "store",
    "dispatcher",
    "reducer"
  ],
  "lint-staged": {
    "*.js": "lint_one"
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react-immutable-proptypes": "^2.1.0",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mocha": "^4.0.1",
    "proxyquire": "^1.8.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.3"
  }
}
