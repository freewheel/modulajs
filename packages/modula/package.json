{
  "name": "modula",
  "version": "1.0.0",
  "description": "a simple intuitive state management framework",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "yarn run build",
    "build": "babel src -d dist --ignore '**/__tests__/*'",
    "lint": "eslint --cache --cache-location node_modules/.cache src",
    "test": "find src -type d -name '__tests__' | xargs mocha --require babel-register",
    "test_one": "mocha --require babel-register",
    "pipeline": "yarn run lint && yarn test && yarn build",
    "watch": "watch 'yarn run pipeline' src",
    "clean": "rimraf dist/",
    "format": "prettier --write \"src/**/*.{js,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:freewheel/modulajs.git"
  },
  "keywords": [
    "modula",
    "next"
  ],
  "author": "FreeWheel",
  "license": "Apache",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-default": "^7.0.0-alpha.20",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "chai": "*",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.69.0",
    "immutable": "^3.8.2",
    "mocha": "^5.0.5",
    "prettier": "^1.11.1",
    "proptypes": "^1.1.0",
    "react-immutable-proptypes": "^2.1.0",
    "rimraf": "^2.6.2",
    "sinon": "*",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "ramda": "^0.25.0",
    "redux": "^3.7.2"
  }
}
